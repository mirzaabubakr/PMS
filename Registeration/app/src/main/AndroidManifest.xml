<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.registeration">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        tools:ignore="ScopedStorage" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="PMS"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Registeration"
        tools:ignore="RtlEnabled"
        tools:targetApi="honeycomb">
        <activity android:name=".PTApplicationCRUDActivity"></activity>
        <activity android:name=".ViewDBActivity" />
        <activity android:name=".ChangePasswordActivity" />
        <activity android:name=".ChangeProjectTAActivity" />
        <activity android:name=".PUDActivity" />
        <activity android:name=".AddActivity" />
        <activity android:name=".ListActivity" />
        <activity android:name=".RUDActivity" />
        <activity android:name=".ResourcesActivity" />
        <activity android:name=".ProjectActivity" />
        <activity android:name=".FormActivity" />
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>